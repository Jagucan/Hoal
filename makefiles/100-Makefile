CC     = gcc
SRC    = school.c main.c
OBJ    = $(SRC:.c=.o)
NAME   = school
RE     = rm -f 
CFLAGS = -Wall -Werror -Wextra -pedantic
RE     = $(RM)
OBJ_S  = school.c
OBJ_M  = main.c

all : $(SRC:.c=.o)
	$(CC) $(SRC:.c=.o) -o $(NAME)

$(OBJ_S) : school.c m.h


%(OBJ_M) : main.c m.h


.PHONY : clean oclean fclean re all

clean :
	$(RM) *~ $(NAME)

oclean :
	$(RM) *.o

fclean : clean oclean

re : oclean all
