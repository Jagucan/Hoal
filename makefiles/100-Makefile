CC     = gcc
SRC    = school.c main.c
OBJ    = $(SRC:.c=.o)
NAME   = school
RE     = rm -f 
CFLAGS = -Wall -Werror -Wextra -pedantic
RE     = $(MR)

all : $(SRC:.c=.o)
	$(CC) $(SRC:.c=.o) -o $(NAME)

school.o : school.c m.h


main.o : main.c m.h


.PHONY : clean oclean fclean re all

clean :
	$(RM) *~ $(NAME)

oclean :
	$(RM) *.o

fclean : clean oclean

re : oclean all
